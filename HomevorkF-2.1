#include <iostream>
#include <cmath>
using namespace std;
double counter=0;
bool luki(int x){
        int a=x/100000%10,b=x/10000%10,c=x/1000%10,d=x/100%10,e=x/10%10,f=x%10;
    if(abs((a+b+c)-(d+e+f))==0){
        return 1;
        }
    else{
        return 0;
        }
    }
void tiket(int x){
    if(luki(x+1)||luki(x-1)){
        cout<<"Yes"<<endl;
        }
    else{
        cout<<"No"<<endl;
        }
}
int main()
{
int n,buff;
cin>>n;
for(int i=0;i<n;i++){
    cin>>buff;
    tiket(buff);
}
}
